; create projections from a volume

; --- input ---

FR G
[vol]../reference_volume   ; input volume

FR G
[refang]refangles   ; reference angles doc file

; --- output ---

FR G
[prj]prj/prj***    ; template for output projections

; ----------- END BATCH HEADER -------------------

VM
mkdir -p prj

FI [width]
[vol]
12

[radius] = INT([width]) - 1

UD N,[nangles]
[refang]

PJ 3Q
[vol]
[radius]
1-[nangles]
[refang]
[prj]

EN D
