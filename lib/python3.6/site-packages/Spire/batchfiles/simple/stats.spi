; stats.spi : get image statistics


; --- input files ---
FR G
[img]img001    ; input image

; --- output files ---

FR G
[stat]stat001    ; output doc file

; --------- END BATCH HEADER --------------

DE     ; delete the doc file if it already exists
[stat]

FS [max],[min],[avg],[std]
[img]

SD /      max          min           avg       std_dev
[stat]

SD 1, [max],[min],[avg],[std]
[stat]

EN D
