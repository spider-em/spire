<Configuration>

<Main>
  <title>Common Lines</title>
  <image>pics/cobweb.gif</image>
  <helpurl>http://www.wadsworth.org/spider_doc/spider/docs/spire/index.html</helpurl>
</Main>


<Locations>
  <location>../batchfiles/Commonlines</location>
</Locations>

<Dialogs>
  <dialog>
  <name>Generate data</name>
  <title>phantom data</title>
  <dir>.</dir>
  <help>http://www.wadsworth.org/spider_doc/spider/docs/spire/index.html</help>
    <button>
      <label>generate random angles</label>
      <buttontext>weightedrandomangles.cor</buttontext>
      <proc>weightedrandomangles.cor</proc>
    </button>
    <button>
      <label>generate CTF profiles</label>
      <buttontext>trapctf.cor</buttontext>
      <proc>trapctf.cor</proc>
    </button>
    <button>
      <label>generate reprojections and apply CTF</label>
      <buttontext>phantom.cor</buttontext>
      <proc>phantom.cor</proc>
    </button>
    <button>
      <label>get image statistics</label>
      <buttontext>getstats.cor</buttontext>
      <proc>getstats.cor</proc>
    </button>
    <button>
      <label>add noise</label>
      <buttontext>addnoise.cor</buttontext>
      <proc>addnoise.cor</proc>
    </button>
  </dialog>
  <dialog>
  <name>Common lines</name>
  <title>main directory</title>
  <dir>.</dir>
  <help>http://www.wadsworth.org/spider_doc/spider/docs/spire/index.html</help>
    <button>
      <label>CTF-correct particles</label>
      <buttontext>ctfcorr.cor</buttontext>
      <proc>ctfcorr.cor</proc>
    </button>
    <button>
      <label>low-pass filter particles</label>
      <buttontext>filter.cor</buttontext>
      <proc>filter.cor</proc>
    </button>
    <button>
      <label>reference-free alignment</label>
      <buttontext>apsr4class.cor</buttontext>
      <proc>apsr4class.cor</proc>
    </button>
    <button>
      <label>K-means classification</label>
      <buttontext>classify_km.cor</buttontext>
      <proc>classify_km.cor</proc>
    </button>
    <button>
      <label>generate class averages</label>
      <buttontext>classavg.cor</buttontext>
      <proc>classavg.cor</proc>
    </button>
    <button>
      <label>calculate 2D resolution</label>
      <buttontext>rescalc.cor</buttontext>
      <proc>rescalc.cor</proc>
    </button>
    <button>
      <label>pick class-averages</label>
      <buttontext>jweb</buttontext>
      <proc><prog>java -Xmx512m -Djava.util.prefs.syncInterval=2000000 web/StartWeb</prog></proc>
    </button>
    <button>
      <label>perform common lines</label>
      <buttontext>comlines.cor</buttontext>
      <proc>comlines.cor</proc>
    </button>
    <button>
      <label>align volumes</label>
      <buttontext>volalign.cor</buttontext>
      <proc>volalign.cor</proc>
    </button>
    <button>
      <label>perform MSA on volumes</label>
      <buttontext>volmsa.cor</buttontext>
      <proc>volmsa.cor</proc>
    </button>
  </dialog>
  <dialog>
  <name>Refinement</name>
  <title>Refinement</title>
  <dir>Refinement</dir>
  <help>http://www.wadsworth.org/spider_doc/spider/docs/techs/recon/mr.html#REF</help>
    <button>
      <label>Assemble files required for refinement</label>
      <buttontext>copyin.cor</buttontext>
      <proc>copyin.cor</proc>
    </button>
    <button>
      <label>Edit refinement parameters</label>
      <buttontext>refine_settings.pam</buttontext>
      <proc>refine_settings.pam</proc>
    </button>
    <group name="Refinement">
    <button>
      <label>Start refinement with PubSub</label>
      <buttontext>pub_refine.pam</buttontext>
      <proc>pub_refine.pam</proc>
    </button>
    <button>
      <label>Start refinement (no PubSub)</label>
      <buttontext>refine.pam</buttontext>
      <proc>refine.pam</proc>
    </button>
    </group>
  </dialog>
</Dialogs>

<Directories>
<dir name=".">
weightedrandomangles.cor,trapctf.cor,plot.gnu,phantom.cor,getstats.cor,addnoise.cor,ctfcorr.cor,filter.cor,apsr4class.cor,counter.cor,classify_km.cor,eigenhist.py,classavg.cor,rescalc.cor,frcmask.cor,frcrescalc.cor,sort_avgs.cor,get_avgs.cor,comlines.cor,.montagefromdoc,.verifybyview,volalign.cor,volmsa.cor</dir>
<dir name="Refinement">
Refinement/copyin.cor,Refinement/endmerge.pam,Refinement/endrefine.pam,Refinement/enhance.pam,Refinement/grploop.pam,Refinement/mergegroups.pam,Refinement/prepare.pam,Refinement/pub_refine_doc_sync.pam,Refinement/pub_refine.pam,Refinement/pub_refine_start.pam,Refinement/pub_refine_wait.pam,Refinement/refine.pam,Refinement/refine_settings.pam,Refinement/saveresp.pam,Refinement/smangloop.pam</dir>
</Directories>
</Configuration>
